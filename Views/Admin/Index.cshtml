@model IEnumerable<Odev.Models.Model.Kategori>
@{
    ViewBag.Title = "Admin Ana Sayfa";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



        <div class="row">
            <div class="col-lg-12">
                <h3 class="page-header"><i class="fa fa-laptop"></i> Admin Paneli</h3>

               
                <ol class="breadcrumb">
                   
                </ol>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                <div class="info-box blue-bg">
                    <i class="fa fa-file-text"></i>
                    <div class="count">@ViewBag.BlogSay</div>
                    <div class="title">Blog Sayısı</div>
                </div>
                
            </div>
            

            <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                <div class="info-box brown-bg">
                    <i class="fa fa-list"></i>
                    <div class="count">@ViewBag.KategoriSay</div>
                    <div class="title">Kategori Sayısı</div>
                </div>
                
            </div>
            

            <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                <div class="info-box dark-bg">
                    <i class="fa fa-car"></i>
                    <div class="count">@ViewBag.UrunSay</div>
                    <div class="title">Araç Sayısı</div>
                </div>
                
            </div>
           

            <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12">
                <div class="info-box green-bg">
                    <i class="fa fa-comment"></i>
                    <div class="count">@ViewBag.YorumSay</div>
                    <div class="title">Yorum Sayısı</div>
                </div>
                
            </div>
            

        </div>
        


        
    <div class="text-right">
        <div class="credits">
            
